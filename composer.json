{
  "name": "pimcore/payment-provider-unzer",
  "license": "GPL-3.0+",
  "type": "pimcore-bundle",
  "description": "Pimcore Payment Provider - Unzer (Heidelpay)",
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "unzerdev/php-sdk": "^3.3.0",
    "pimcore/pimcore": "^11.0",
    "pimcore/ecommerce-framework-bundle": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "Pimcore\\Bundle\\EcommerceFrameworkBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Pimcore\\Bundle\\EcommerceFrameworkBundle\\PimcorePaymentProviderUnzerBundle"
      ]
    }
  }
}
